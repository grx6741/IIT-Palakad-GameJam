config_version=5

[application]

config/name="IIT-Palakad"
run/main_scene="res://Scenes/main.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720

[file_customization]

folder_colors={
"res://Assets/": "purple",
"res://Audio/": "blue",
"res://Levels/": "teal",
"res://Scenes/": "green",
"res://Scripts/": "yellow",
"res://Shaders/": "orange",
"res://UI/": "red",
"res://addons/": "gray"
}

[filesystem]

import/blender/enabled=false

[importer_defaults]

mp3={
"bar_beats": 4,
"beat_count": 0,
"bpm": 0,
"loop": true,
"loop_offset": 0
}

